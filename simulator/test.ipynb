    "        self.vcc.state = 1\n",
    "        self.code = []\n",
    "        \n",
    "    def __str__(self) -> str:\n",
    "        return \"board\"\n",
    "    \n",
    "    def pinmode(self, pin: int, mode: int):\n",
    "        self.pins[pin].mode = mode # 0 for input 1 for output\n",
    "\n",
    "    def digitalread(self, pin: int):\n",
