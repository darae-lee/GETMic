    "                pass\n",
    "            elif pin.mode == INPUT:\n",
    "                # print(\"input\", pin)\n",
    "                # send travel in reverse way...?\n",
    "                # for other, startin from the gnd direction (actually vcc)\n",
    "                pass\n",
    "        self.gnd.state = -1\n",
