    "    def travel(self, state=-1, dir=OUTPUT):\n",
    "        # print(\"I am PIN!\", self.name)\n",
    "        if state == -1:\n",
    "            state = self.state\n",
    "        else:\n",
    "            self.state = state\n",
    "        for con in self.connect:\n",
